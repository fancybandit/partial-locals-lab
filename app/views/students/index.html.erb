<%= form_tag(students_path, method: :get) do %>
    <%= text_field_tag :search, params[:search] %>
    <%= submit_tag "Search", name: nil %>
<% end %>
<% if @students %>
    <ul>
        <% @students.each do |student| %>
            <li><%= render partial: "student", locals: {student: student} %></li>
        <% end %>
    </ul>
<% end %>